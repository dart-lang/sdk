class MapLike<K, V> {
  V? get(Object key) => _map[key];
  V put(K key, V value) => _map[key] = value;
  final Map<K, V> _map = {};
}

class MapLike2<K, V> {
  MapLike2(this.defaultValue);
  V get(Object key) => _map[key] ?? defaultValue;
  V put(K key, V value) => _map[key] = value;
  final Map<K, V> _map = {};
  final V defaultValue;
}

