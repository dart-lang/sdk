extension Extension<K, V> on MapLike<K, V> {
  V? operator [](Object key) => get(key);
  void operator []=(K key, V value) => put(key, value);
}

extension Extension2<K, V> on MapLike2<K, V> {
  V operator [](Object key) => get(key);
  void operator []=(K key, V value) => put(key, value);
}

