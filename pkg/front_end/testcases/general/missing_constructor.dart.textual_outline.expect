class Super {
  Super._();
}

class Sub extends Super {
  Sub() : super();
  Sub.foo() : super.foo();
}

class Bad {
  Bad.foo() : this();
  Bad.bar() : this.baz();
}

class M {}

class MixinApplication extends Super with M {
  MixinApplication() : super();
  MixinApplication.foo() : super.foo();
}

main() {}
