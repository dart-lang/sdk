      },
      "steps": [
        {
          "name": "build dart",
          "script": "tools/build.py",
          "arguments": ["runtime_kernel"]
        },
        {
          "name": "vm tests",
          "arguments": [
            "-ndartk-${system}-${mode}-${arch}"],
          "fileset": "vm-kernel",
          "shards": 10
        }
      ]
    },
    {
