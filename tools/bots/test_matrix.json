    {
      "builders": [
        "vm-kernel-precomp-obfuscate-linux-release-x64"
      ],
      "meta": {
        "description": "This configuration is used by the obfuscated vm kernel precomp builders."
      },
      "steps": [
        {
          "name": "build dart",
          "script": "tools/build.py",
          "arguments": [
