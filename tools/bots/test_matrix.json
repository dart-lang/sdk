        {
          "name": "package unit tests",
          "arguments": [
            "--use-sdk",
            "--compiler=none",
            "--runtime=vm",
            "--checked"
          ],
          "tests": ["pkg"]
        },
        {
          "name": "third_party/pkg_tested unit tests",
          "arguments": [
